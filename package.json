{
   "name": "ngx-codetab",
   "version": "1.0.1",
   "scripts": {
      "ng": "ng",
      "start": "ng serve",
      "build": "echo \"Error: Please specify which project you want to build via npm run build{:lib|:demo}\" && exit 1",
      "build:lib": "ng build ngx-codetab",
      "build:demo": "ng build ngx-codetab-demo",
      "build:prod": "npm run build:lib:prod && npm run build:demo:prod",
      "build:lib:prod": "ng build ngx-codetab --prod",
      "build:demo:prod": "ng build ngx-codetab-demo --prod --base-href /ngx-codetab/",
      "test": "npm run test:lib && npm run test:demo",
      "test:lib": "ng test ngx-codetab",
      "test:demo": "ng test ngx-codetab-demo",
      "lint": "ng lint",
      "format": "prettier --write \"**/*.*\""
   },
   "author": {
      "email": "JReed129@gmail.com",
      "name": "Jeremy Reed",
      "url": "http://jr33d.github.io"
   },
   "repository": {
      "type": "GitHub",
      "url": "https://github.com/jr33d/ngx-codetab"
   },
   "bugs": {
      "url": "https://github.com/jr33d/ngx-codetab/issues"
   },
   "keywords": [
      "Angular",
      "Tabs",
      "Code",
      "Component",
      "ngx"
   ],
   "license": "The Unlicense",
